@model CucuruchoCafe.Models.SucursalesModels
@using CucuruchoCafe.Models
@using System.Data;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Concentradora";
}


@if (Request.IsAuthenticated)
{
    

    <div class=" container-scroller">
        <!-- partial:partials/_navbar.html -->
        @Html.Partial("_navbar")
        <div class="container-fluid">
            <div class="row row-offcanvas row-offcanvas-right">
                @Html.Partial("_sidebar")
            </div>
                @{@Html.Partial("_Concentradora");}
             
                    <center>
                        <table>
                            <tr>
                                <td><a href="#" type="button" id="btnBack" class="btn btn-success mr-2"> << </a></td>
                                <td>
                                    <div>
                                        <input type="hidden" id="contTabla" name="contTabla" value="1" />
                                    </div>
                                </td>
                                <td><a href="#" type="button" id="btnNext" class="btn btn-success mr-2"> >> </a></td>

                            </tr>
                        </table>
                    </center>
        
              
        </div>

    </div>

}
else
{
    @Html.Action("Login", "Account")
}

